<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">
    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>servlet.TestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BirthdayServlet</servlet-name>
        <servlet-class>servlet.BirthdayServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UConnServlet</servlet-name>
        <servlet-class>servlet.UConnServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DiscoServlet</servlet-name>
        <servlet-class>servlet.DiscoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchLightSServlet</servlet-name>
        <servlet-class>servlet.SearchLightSServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchLightRServlet</servlet-name>
        <servlet-class>servlet.SearchLightRServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SearchLightRServlet</servlet-name>
        <url-pattern>/servlet/SearchLightRServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchLightSServlet</servlet-name>
        <url-pattern>/servlet/SearchLightSServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UConnServlet</servlet-name>
        <url-pattern>/servlet/UConnServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DiscoServlet</servlet-name>
        <url-pattern>/servlet/DiscoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestServlet</servlet-name>
        <url-pattern>/servlet/TestServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BirthdayServlet</servlet-name>
        <url-pattern>/servlet/BirthdayServlet</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
